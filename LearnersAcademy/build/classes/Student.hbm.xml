<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.simplilearn">
    <class name="com.simplilearn.Student" table="student_mst">
        <id name="STUDENTID" type="long" column="STUDENTID">
            <generator class="identity"/>
        </id>
        <property name="firstname" type="string" column="FNAME"/>
        <property name="lastname" type="string" column="LNAME"/>
    </class>
</hibernate-mapping>
